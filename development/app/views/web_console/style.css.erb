.console .pos-absolute { position: absolute; }
.console .pos-fixed { position: fixed; }
.console .pos-right { right: 0; }
.console .border-box { box-sizing: border-box; }
.console .layer { width: 100%; height: 100%; }
.console .layer.console-outer { z-index: 1; }
.console .layer.resizer { z-index: 2; position: absolute; top: 0; }
.console { position: fixed; left: 0; bottom: 0; width: 100%; height: 130px; min-height: 50px; max-height: 480px; padding: 10px 10px 40px 10px; margin: 0; background: none repeat scroll 0% 0% #41b6a6; z-index: 9998; }
.console .slimScrollDiv .console-outer { padding-right: 25px; }
.console .console-outer { overflow: auto; }
.console .console-inner { font-family: monospace; font-size: 11px; width: 100%; height: 100%; background: #41b6a6; }
.console .console-prompt-box { color: #FFF; }
.console .console-message { color: #2e877b; margin: 0; border: 0; white-space: pre-wrap; background-color: #41b6a6; padding: 0; }
.console .console-message.error-message { color: #fc9; }
.console .console-focus .console-cursor { background: #FEFEFE; color: #41b6a6; font-weight: bold; }
.console .resizer { background: #41b6a6; width: 100%; height: 4px; cursor: ns-resize; }
.console .button { cursor: pointer; border-radius: 1px; font-family: monospace; font-size: 13px; width: 14px; height: 14px; line-height: 14px; text-align: center; color: #ccc; }
.console .button:hover { background: #666; color: #fff; }
.console .button.close-button:hover { background: #d64841; }
.console .clipboard { height: 0px; padding: 0px; margin: 0px; width: 0px; margin-left: -1000px; }
.console .console-prompt-label { display: inline; color: #FFF; background: none repeat scroll 0% 0% #41b6a6; border: 0; padding: 0; }
.console .console-prompt-display { display: inline; color: #FFF; background: none repeat scroll 0% 0% #41b6a6; border: 0; padding: 0; }
.console.full-screen { height: 100%; }
.console.full-screen .console-outer { padding-top: 3px; }
.console.full-screen .resizer { display: none; }
.console.full-screen .close-button { display: none; }
